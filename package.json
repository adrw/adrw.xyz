{
  "private": true,
  "name": "andrew.fm",
  "scripts": {
    "start": "npm run lint && gatsby develop",
    "build": "npm run lint && gatsby build",
    "lint": "prettier --write --config package.json \".{/notes/**/,/pages/**/,/posts/**/,/projects/**/,/src/**/,/}*.{md,mdx,css,sass,less,json,js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "dayjs": "1.8.14",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "emotion-theming": "^10.0.10",
    "@mdx-js/mdx": "1.0.18",
    "@mdx-js/react": "1.0.16",
    "gatsby": "^2.3.24",
    "gatsby-image": "2.0.41",
    "gatsby-mdx": "0.6.3",
    "gatsby-plugin-mdx": "1.0.1",
    "gatsby-plugin-netlify": "2.0.16",
    "gatsby-plugin-netlify-cms": "4.0.0",
    "gatsby-plugin-offline": "2.1.0",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-redirects": "1.0.0",
    "gatsby-plugin-sharp": "2.0.36",
    "gatsby-remark-copy-linked-files": "2.0.12",
    "gatsby-remark-images": "3.0.11",
    "gatsby-remark-relative-images": "0.2.2",
    "gatsby-source-filesystem": "2.0.33",
    "gatsby-theme-digital-garden": "^0.0.22",
    "gatsby-theme-digital-garden-blog": "^0.0.22",
    "gatsby-theme-digital-garden-portfolio": "^0.0.22",
    "gatsby-transformer-remark": "2.3.12",
    "gatsby-transformer-sharp": "2.1.19",
    "netlify-cms": "2.9.1",
    "prettier": "1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "5.2.1",
    "theme-ui": "^0.0.8"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
